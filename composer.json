{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "defuse/php-encryption": "^2.3",
        "drupal/ace_editor": "^1.3",
        "drupal/address": "^1.9",
        "drupal/admin_content_notification": "^3.8",
        "drupal/admin_toolbar": "^3.0",
        "drupal/admin_toolbar_links_access_filter": "^3.0",
        "drupal/blazy": "^2.5",
        "drupal/bootstrap": "^3.23",
        "drupal/captcha": "^1.2",
        "drupal/charts": "^3.3",
        "drupal/checklistapi": "^2.0",
        "drupal/contact_emails": "^1.20",
        "drupal/contact_storage": "^1.1",
        "drupal/core": "^9.2",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
	"drupal/core-vendor-hardening": "^9.2",
        "drupal/ctools": "^3.7",
        "drupal/dropzonejs": "^2.5",
        "drupal/dropzonejs_eb_widget": "^2.5",
        "drupal/entity_embed": "^1.2",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/extra_field_plus": "^2.0",
        "drupal/facets": "^1.8",
        "drupal/field_token_value": "^2.0",
        "drupal/file_mdm": "^2.1",
        "drupal/fontawesome": "^2.19",
        "drupal/geofield": "^1.35",
        "drupal/leaflet": "^2.1",
        "drupal/mailsystem": "^4.3",
        "drupal/menus_attribute": "^1.2",
        "drupal/metatag": "^1.16",
        "drupal/metatag_open_graph": "^1.16",
        "drupal/metatag_views": "^1.16",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/office_hours": "^1.5",
        "drupal/paragraphs": "^1.12",
        "drupal/phone_international": "^3.1",
        "drupal/profile": "^1.3",
        "drupal/redirect": "^1.6",
        "drupal/schema_metatag": "^2.2",
        "drupal/scn": "^1.5",
        "drupal/search_api": "^1.21",
        "drupal/search_api_autocomplete": "^1.5",
        "drupal/search_api_solr": "^4.2",
        "drupal/search_api_solr_nlp": "^1.1",
        "drupal/seo_checklist": "^5.1",
        "drupal/simple_mobile_menu": "^2.1",
        "drupal/simple_sitemap": "^4.0",
        "drupal/simple_sitemap_engines": "^4.0",
        "drupal/simplify": "^1.2",
        "drupal/slick": "^2.4",
        "drupal/slick_views": "^2.5",
        "drupal/social_api": "^3.0",
        "drupal/social_auth": "^3.0",
        "drupal/social_auth_facebook": "^3.0",
        "drupal/super_login": "^1.6",
        "drupal/superfish": "^1.4",
        "drupal/svg_image": "^1.15",
        "drupal/svg_image_responsive": "^1.15",
        "drupal/swiftmailer": "^2.0",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/telephone_validation": "^2.3",
        "drupal/thank_you_commenter": "^2.0",
        "drupal/token": "^1.10",
        "drupal/token_filter": "^1.3",
        "drupal/token_views_filter": "^1.5",
        "drupal/toolbar_anti_flicker": "^9.3",
        "drupal/toolbar_menu": "^2.2",
        "drupal/unique_entity_title": "^1.2",
        "drupal/username_enumeration_prevention": "^1.2",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_add_button": "^2.0",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_conditional": "^1.4",
        "drupal/views_row_insert": "^2.0",
        "drupal/views_send": "^1.5",
        "drupal/viewsreference": "^1.7",
        "drupal/votingapi": "^3.0@beta",
        "drupal/votingapi_widgets": "^1.0@alpha",
        "drupal/webform": "^6.1",
        "drupal/webform_bootstrap": "^6.1",
        "drupal/webform_submission_log": "^6.1",
        "drupal/webform_templates": "^6.1",
        "drupal/webform_ui": "^6.1",
        "drush/drush": "^10.6",
        "league/oauth2-google": "^4.0",
        "npm-asset/ace-builds": "^1.4",
        "oomphinc/composer-installers-extender": "^2.0",
        "pear/console_getopt": "^1.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "./core": [
                "type:drupal-core"
            ],
            "./libraries/{$name}": [
                "type:drupal-library"
            ],
            "./modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "./profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "./themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "./modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "./profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "./themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
